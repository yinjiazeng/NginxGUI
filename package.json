{
  "name": "NginxGui",
  "version": "1.0.0",
  "description": "NginxGui",
  "main": "main.js",
  "author": "yinjiazeng",
  "license": "ISC",
  "scripts": {
    "start": "electron .",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress",
